@model EducationYear
<h1 class="alert alert-success text-center">Edit Education Year</h1>

<div class="row">
    <form class="col-4" method="post" asp-controller="EducationYears" asp-action="EditCurrent" enctype="multipart/form-data">

        <input type="hidden" asp-for="Id" />

        <div>
            <label asp-for="Name"></label>
            <input type="text" asp-for="Name" class="form-control" />
            <span asp-validation-for="Name" class="text-danger"></span>
        </div>

        <button type="submit" class="btn btn-primary mt-2">Edit</button>
    </form>

</div>

<a asp-controller="Education Year" asp-action="GetHomeView">Back to List</a>


<partial name="_TSContact" />
